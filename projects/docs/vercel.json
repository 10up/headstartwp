{
    "version": 2,
    "builds": [
        {
        "src": "lib/index.js",
        "use": "@vercel/node"
        }
    ],
    "functions": {
      "api/index.js": {
        "includeFiles": "_docs/**"
      }
    },
    "routes": [
        {
          "src": "/(.*)",
          "dest": "lib/index.js"
        }
      ]
  }