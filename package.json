{
  "name": "@10up/headless_framework",
  "version": "1.0.0",
  "description": "Monorepo for headless framework",
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build --filter=./packages/*",
    "test": "npm run build:packages && turbo run test",
    "lint": "eslint .",
    "dev": "npm run build:packages && turbo run dev --parallel",
    "prepare": "husky install",
    "typedoc": "typedoc",
    "typedoc:watch": "typedoc --watch",
    "publish": "npm run build:packages && npx changeset publish"
  },
  "author": "Nícholas André <nicholas.andre@10up.com>",
  "workspaces": [
    "packages/*",
    "projects/*"
  ],
  "devDependencies": {
    "@10up/eslint-config": "^2.3.7",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^6.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jest": "^25.2.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "turbo": "^1.2.11",
    "jest-environment-jsdom": "^29.0.3",
    "jest": "^29.0.3"
  },
  "nextBundleAnalysis": {
    "buildOutputDirectory": "./projects/wp-nextjs/.next",
    "budget": 148480,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
