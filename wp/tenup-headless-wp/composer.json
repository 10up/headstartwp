{
  "name": "10up/tenup-headless-wp",
  "description": "10up Headless WordPress API Plugin",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "10up",
      "email": "info@10up.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "cheprasov/php-redis-client": "^1.10",
    "predis/predis": "^2.0"
  },
  "require-dev": {
    "10up/phpcs-composer": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "HeadlessWP\\": "includes/classes/"
    }
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf ."
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
