{
  "name": "@headstartwp/blocks-primitives",
  "version": "0.0.1",
  "description": "A set of Gutenberg Primitives for authoring react components that can be shared with the block editor",
  "homepage": "https://github.com/10up/headstartwp/blob/develop/packages/primitives/README.md",
  "license": "MIT",
  "author": "10up <info@10up.com> (https://10up.com/)",
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/mjs/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js",
      "types": "./dist/mjs/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "dev": "concurrently \"npm run watch:ts:cjs\" \"npm run watch:ts\"",
    "watch:ts:cjs": "npm run ts:cjs -- --watch",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.1",
    "typescript": "^5.0.4",
    "whatwg-fetch": "^3.6.2",
    "@wordpress/block-editor": "^12.17.0",
    "@wordpress/data": "^9.19.0"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "@wordpress/block-editor": "^12.17.0",
    "@wordpress/data": "^9.19.0"
  },
  "peerDependenciesMeta": {
    "@wordpress/block-editor": {
      "optional": true
    },
    "@wordpress/data": {
      "optional": true
    }
  }
}
