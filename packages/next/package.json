{
  "name": "@headstartwp/next",
  "version": "1.0.0-next.4",
  "description": "`@headstartwp/next` is the Next.js bindings for the headless framework.",
  "homepage": "https://github.com/10up/headless/blob/develop/packages/next/README.md",
  "license": "MIT",
  "author": "10up <info@10up.com> (https://10up.com/)",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/mjs/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./middlewares": {
      "types": "./dist/middlewares/index.d.ts",
      "default": "./dist/middlewares/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "middleware.d.ts",
    "middleware.js",
    "config.d.ts",
    "config.js"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run ts",
    "ts": "tsc -b",
    "dev": "npm run watch:ts",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {
    "@headstartwp/core": "^1.0.0-next.3",
    "modify-source-webpack-plugin": "^4.0.1",
    "html-react-parser": "^1.4.3",
    "loader-utils": "^3.2.0",
    "schema-utils": "^4.0.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "copy-webpack-plugin": "^10.2.4",
    "expect-type": "^0.15.0",
    "jest": "^29.0.3",
    "next-router-mock": "^0.9.1",
    "node-mocks-http": "^1.11.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "next": ">= 12.0.0",
    "react": ">= 17.0.2"
  }
}
