{
  "name": "@10up/headless-next",
  "version": "0.0.1-alpha.8",
  "description": "",
  "module": "dist/mjs/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./middlewares": {
      "import": "./dist/mjs/middlewares/index.js",
      "require": "./dist/cjs/middlewares/index.js"
    },
    "./config": {
      "import": "./dist/mjs/config/index.js",
      "require": "./dist/cjs/config/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "dev": "npm run ts -- --watch",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "test": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@10up/headless-core": "^0.0.1-alpha.8",
    "@justinribeiro/lite-youtube": "^1.3.1",
