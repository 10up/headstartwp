{
  "name": "@10up/headless-next",
  "version": "0.0.1-alpha.28",
  "description": "",
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": { 
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./middlewares": {
      "require": "./dist/cjs/middlewares/index.js",
      "import": "./dist/mjs/middlewares/index.js"
    },
    "./config": { 
      "require": "./dist/cjs/config/index.js",
      "import": "./dist/mjs/config/index.js"
    },
    "./config/loader": "./config-loader.js",
    "./webpack-loader": "./webpack-loader.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "config-loader.js",
    "webpack-loader.js"
  ],
  "sideEffects": "false",
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "dev": "npm run build -- --watch",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "test": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@10up/headless-core": "^0.0.1-alpha.28",
    "@10up/headless-webpack-loader": "^0.0.1-alpha.25",
    "@justinribeiro/lite-youtube": "^1.3.1",
    "html-react-parser": "^1.4.3",
    "loader-utils": "^3.2.0",
    "next-transpile-modules": "^9.0.0",
    "schema-utils": "^4.0.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.6.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "copy-webpack-plugin": "^10.2.4",
    "jest": "^27.4.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "next": "^12.0.0",
    "react": "^17.0.2"
  }
}
