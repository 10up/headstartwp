{
  "name": "@10up/headless-next",
  "version": "1.0.0",
  "description": "",
  "module": "dist/mjs/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./middlewares": {
      "import": "./dist/mjs/middlewares/index.js",
      "require": "./dist/cjs/middlewares/index.js"
    },
    "./data": {
      "import": "./dist/mjs/data/index.js",
      "require": "./dist/cjs/data/index.js"
    },
    "./components": {
      "import": "./dist/mjs/components/index.js",
      "require": "./dist/cjs/components/index.js"
    },
    "./blocks": {
      "import": "./dist/mjs/blocks/index.js",
      "require": "./dist/cjs/blocks/index.js"
    },
    "./utils": {
      "import": "./dist/mjs/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    }
  },
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "dev": "npm run ts -- --watch",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "test": "jest --passWithNoTests"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@10up/headless-core": "^1.0.0",
    "html-react-parser": "^1.4.3",
    "swr": "^1.0.1",
    "@justinribeiro/lite-youtube": "^1.3.1"
  },
  "devDependencies": {
    "jest": "^27.4.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0"
  },
  "peerDependencies": {
    "next": "^12.0.0",
    "react": "^17.0.2"
  }
}
