{
  "name": "@10up/headless-next",
  "version": "0.5.0-next.3",
  "description": "",
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/mjs/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./middlewares": {
      "require": "./dist/cjs/middlewares/index.js",
      "import": "./dist/mjs/middlewares/index.js"
    },
    "./config": {
      "require": "./dist/cjs/config/index.js",
      "import": "./dist/mjs/config/index.js"
    },
    "./config/loader": "./config-loader.js",
    "./webpack-loader": "./webpack-loader.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "config-loader.js",
    "webpack-loader.js"
  ],
  "sideEffects": "false",
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "dev": "concurrently \"npm run watch:ts:cjs\" \"npm run watch:ts\"",
    "watch:ts:cjs": "npm run ts:cjs -- --watch",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest",
    "lint": "eslint src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@10up/headless-core": "^0.5.0-next.5",
    "html-react-parser": "^1.4.3",
    "loader-utils": "^3.2.0",
    "next-transpile-modules": "^10.0.0",
    "schema-utils": "^4.0.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "copy-webpack-plugin": "^10.2.4",
    "jest": "^29.0.3",
    "node-mocks-http": "^1.11.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "next": ">= 12.0.0",
    "react": ">= 17.0.2"
  }
}
