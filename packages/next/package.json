    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.6.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "next": "^12.0.0",
    "react": "^17.0.2"
