    "jest": "^27.2.1",
    "msw": "^0.35.0",
    "node-fetch": "^2.6.5",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@wordpress/url": "^3.2.2",
    "path-to-regexp": "^6.2.0"
