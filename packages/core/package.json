{
  "name": "@10up/headless-core",
  "version": "0.4.3",
  "description": "",
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/mjs/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./utils": {
      "require": "./dist/cjs/utils/index.js",
      "import": "./dist/mjs/utils/index.js"
    },
    "./react": {
      "require": "./dist/cjs/react/index.js",
      "import": "./dist/mjs/react/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": "false",
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "dev": "concurrently \"npm run watch:ts:cjs\" \"npm run watch:ts\"",
    "watch:ts:cjs": "npm run ts:cjs -- --watch",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/dom": "^8.6.0",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^29.0.3",
    "@types/node-fetch": "^2.5.3",
    "@types/sanitize-html": "^2.6.0",
    "jest": "^29.0.3",
    "msw": "^0.35.0",
    "node-fetch": "^2.6.7",
    "ts-jest": "^29.0.1",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@justinribeiro/lite-youtube": "^1.3.1",
    "html-react-parser": "^1.4.3",
    "path-to-regexp": "^6.2.0",
    "react-inspector": "^5.1.1",
    "swr": "^1.2.2",
    "xss": "^1.0.11"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
