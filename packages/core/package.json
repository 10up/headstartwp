{
  "name": "@10up/headless-core",
  "version": "0.6.0-next.3",
  "description": "`@10up/headless-core` is the core package that houses framework-agnostic components and utilities for building headless sites with WordPress.",
  "homepage": "https://github.com/10up/headless/blob/develop/packages/core/README.md",
  "license": "MIT",
  "author": "10up <info@10up.com> (https://10up.com/)",
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/mjs/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js",
      "types": "./dist/mjs/index.d.ts"
    },
    "./utils": {
      "require": "./dist/cjs/utils/index.js",
      "import": "./dist/mjs/utils/index.js",
      "types": "./dist/mjs/utils/index.d.ts"
    },
    "./react": {
      "require": "./dist/cjs/react/index.js",
      "import": "./dist/mjs/react/index.js",
      "types": "./dist/mjs/react/index.d.ts"
    },
    "./test": "./test/server.ts"
  },
  "files": [
    "dist"
  ],
  "sideEffects": "false",
  "scripts": {
    "build": "npm run ts && npm run ts:cjs",
    "ts:cjs": "tsc -b tsconfig-cjs.json",
    "ts": "tsc -b",
    "dev": "concurrently \"npm run watch:ts:cjs\" \"npm run watch:ts\"",
    "watch:ts:cjs": "npm run ts:cjs -- --watch",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/node-fetch": "^2.5.3",
    "expect-type": "^0.15.0",
    "jest": "^29.3.1",
    "msw": "^0.35.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@justinribeiro/lite-youtube": "^1.3.1",
    "html-react-parser": "^3.0.4",
    "path-to-regexp": "^6.2.0",
    "react-inspector": "^6.0.1",
    "swr": "^1.3.0",
    "xss": "^1.0.11"
  },
  "peerDependencies": {
    "react": ">= 17.0.2"
  }
}
