{
  "name": "@headstartwp/core",
  "version": "1.0.0-next.4",
  "description": "`@headstartwp/core` is the core package that houses framework-agnostic components and utilities for building headless sites with WordPress.",
  "homepage": "https://github.com/10up/headless/blob/develop/packages/core/README.md",
  "license": "MIT",
  "author": "10up <info@10up.com> (https://10up.com/)",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "typedocMain": "src/docs-entry-point.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./test": "./test/server.ts"
  },
  "files": [
    "dist",
    "test.d.ts",
    "utils.d.ts",
    "utils.js"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/utils/index.ts --dts --format cjs --minify",
    "ts": "tsc -b",
    "dev": "tsup src/index.ts src/utils/index.ts --format cjs --watch",
    "watch:ts": "npm run ts -- --watch",
    "test": "jest",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/node-fetch": "^2.5.3",
    "expect-type": "^0.15.0",
    "jest": "^29.3.1",
    "msw": "^0.35.0",
    "ts-jest": "^29.0.3",
    "tsup": "^6.7.0",
    "typescript": "^4.4.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0",
    "xss": "^1.0.11",
    "html-react-parser": "^3.0.4"
  },
  "peerDependencies": {
    "react": ">= 17.0.2"
  }
}
